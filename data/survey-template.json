    {
      "name": "id",
      "title": "Identification",
      "elements": [
        {
          "type": "text",
          "name": "idno",
          "title": "Numéro d'identification",
          "placeholder": "IE0xxx"
        },
        {
          "type": "text",
          "name": "titl",
          "title": "Titre",
          "placeholder": "Titre de l’enquête"
        },
        {
          "type": "tagbox",
          "name": "ingeref",
          "title": "Ingénieur.es référent.es",
          "choices": [
            "Amandine STEPHAN ",
            "Aurélie SANTOS",
            "Efi MARKOU",
            "Gaspar MASSIOT",
            "Géraldine CHARRANCE ",
            "Geraldine VIVIER",
            "Patricia THAUVIN",
            "Paul COCHET",
            "Ruxandra BREDA-POPA"
          ],
          "choicesOrder": "random",
          "placeholder": "Ajouter...",
          "allowCustomChoices": true
        },
        {
          "type": "text",
          "name": "website",
          "title": "Site web de l’enquête",
          "inputType": "url",
          "placeholder": "nouvellenquete.site.ined.fr"
        },
        {
          "type": "file",
          "name": "question1",
          "title": "Logo de l’enquête",
          "acceptedTypes": "image/*",
          "filePlaceholder": "Glissez-déposez le logo ici ou cliquez sur le bouton ci-dessous pour sélectionner une image à télécharger."
