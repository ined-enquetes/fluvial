        {
          "type": "paneldynamic",
          "name": "rspStmt",
          "title": "Équipe de production des métadonnées",
          "description": "A remplir par l'équipe Mise à Disposition",
          "templateElements": [
            {
              "type": "text",
              "name": "docDscr@AuthEnty",
              "minWidth": "150px",
              "maxWidth": "50%",
              "title": "NOM Prénom"
            },
            {
              "type": "tagbox",
              "name": "docDscr@AuthEnty@affliliation",
              "minWidth": "150px",
              "maxWidth": "50%",
              "startWithNewLine": false,
              "title": "Affliliation",
              "choices": [
                {
                  "value": "Service des enquêtes et des sondages, Ined",
                  "text": "SES, Ined"
                }
              ],
              "allowCustomChoices": true
            }
          ]
        }
      ]
    },
    {
      "name": "Informations Générales",
